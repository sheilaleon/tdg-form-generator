{
  "arrowParens": "always",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "endOfLine": "auto",
  "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
      "^react$",
      "^next(/.*)?$",
      "next-themes",
      "^@/components/ui/(.*)$",
      "^@/components/(.*)$",
      "^@/lib/(.*)$",
      "^@/hooks/(.*)$",
      "<THIRD_PARTY_MODULES>"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
